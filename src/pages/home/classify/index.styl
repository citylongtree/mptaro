@import "./../../../styles/mixin.styl"
.classify{
    height 310px
    background-color #fff
    padding 32px 20px 20px
    .classify-box{
      wh(97px, 120px)
      margin-right 57px
      padding-bottom 23px
      .classify-img{
        wh(84px, 84px)
        border-radius 30px
        margin 0 5px
      }
      .classify-font{
        font-size 24px
        color #313131
        text-align center
      }
    }
    .classify-box:nth-child(5n+5){
      margin-right 0
    }
  }
