@import "../../styles/mixin.styl"
.logisticsTop{
    border-bottom 14px solid #F8F8F8
    font-size $font-size-4
    color #5C5C5C
    padding-left 23px
    padding-right 23px
}
.address{
    margin-top 38px
    margin-bottom 27px
}
.orderNumber{
    margin-top 28px
    margin-bottom 39px    
}
.trace{
    color #343434
    font-size $font-size-5
    margin 35px 0 0 24px
}
.step{
    margin 40px 0 0 24px
}
.at-timelineitem__content-item{
    font-size $font-size-6
}
.at-timelineitem{
    height 100%
}
.at-timelineitem__content--sub{
    margin-top 23px 
}
.logisticsMain .at-icon-map-pin{
    color #DDDDDD
}

.at-timelineitem:first-child{
    .at-icon,.at-timelineitem__content,.at-timelineitem__content--sub{
        color #FB730D
    }
}
.at-timelineitem__content-item{
    padding-right 10px
}