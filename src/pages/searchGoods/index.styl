@import "./../../styles/mixin.styl"
$box-height = 100px
.search-goods {
  wh(100%, 100vh)
  display flex
  flex-direction column
  .search-goods-box {
    padding 0 30px
    width 100%
    height 114px
    box-sizing border-box
    background-color #fff
  }
  .search-goods-box1 {
    flex 1
    flex-direction column
    width 100%
    display flex
    box-sizing border-box
    .search-goods-btnTop {
      background-color #fff
      height 110px
      width 100%
    }
    .search-goods-btnBottom{
      flex 1
      display flex
      height 100%
      width 100%
    }
  }
  .noOrder{
    width 100vm
  }
}
.at-search-bar__placeholder_wrap{
  -webkit-justify-content:left !important
  justify-content:left !important
}
